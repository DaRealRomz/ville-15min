{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb", "airbnb/hooks", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "import/no-extraneous-dependencies": "off",
      "react/react-in-jsx-scope": "off"
    },
    "root": true
}
